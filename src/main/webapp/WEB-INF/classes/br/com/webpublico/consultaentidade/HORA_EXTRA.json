    "from": " from HoraExtra obj\r\n         inner join contratofp cont on obj.CONTRATOFP_ID = cont.ID\r\n         inner join vinculofp v on cont.ID = v.ID\r\n         inner join matriculafp mat on v.MATRICULAFP_ID = mat.ID\r\n         inner join pessoafisica pf on mat.PESSOA_ID = pf.ID\r\nwhere (\r\n              (not exists(\r\n                      select usuario.id\r\n                      from USUARIOSISTEMA usuario\r\n                      where acessoTodosVinculosRH = 1\r\n                        and id = $USUARIO_ID)\r\n                  and exists(\r\n                       select lot.id\r\n                       from LOTACAOFUNCIONAL lot\r\n                                inner join HIERARQUIAORGANIZACIONAL ho\r\n                                           on lot.UNIDADEORGANIZACIONAL_ID = ho.SUBORDINADA_ID and\r\n                                              ho.TIPOHIERARQUIAORGANIZACIONAL = 'ADMINISTRATIVA'\r\n                       where $DATA_OPERACAO\r\n                           between ho.inicioVigencia\r\n                           and coalesce(ho.fimVigencia, $DATA_OPERACAO)\r\n                         and v.ID = lot.VINCULOFP_ID\r\n                         and substr(ho.CODIGO, 1, 6) = (select substr(vw.CODIGO, 1, 6)\r\n                                                        from vwhierarquiaadministrativa vw\r\n                                                        where vw.SUBORDINADA_ID = $UNIDADE_ADM_ID\r\n                                                          and $DATA_OPERACAO between vw.inicioVigencia\r\n                                                            and coalesce(vw.fimVigencia, $DATA_OPERACAO))\r\n                      and $DATA_OPERACAO\r\n                           between lot.inicioVigencia\r\n                           and coalesce(lot.FINALVIGENCIA, $DATA_OPERACAO)\r\n                   )\r\n                  )\r\n              or exists(\r\n                      select usuario.id\r\n                      from USUARIOSISTEMA usuario\r\n                      where acessoTodosVinculosRH = 1\r\n                        and id = $USUARIO_ID)\r\n          )",
