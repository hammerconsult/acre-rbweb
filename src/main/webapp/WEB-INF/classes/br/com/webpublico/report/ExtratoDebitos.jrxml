	<queryString>
		<![CDATA[SELECT
  COALESCE(pf.nome, pj.razaosocial) AS contribuinte,
  COALESCE(pf.cpf, pj.cnpj) AS cfp_cnpj,
  pj.inscricaoestadual,
  pj.nomefantasia,
  pessoa.email as email,
  (select telefone from telefone where pessoa_id = pessoa.id and rownum = 1) as telefone,
  (SELECT
      trim(e.logradouro) || ', ' || trim(e.numero) || ', ' || trim(e.complemento) || ', ' || trim(e.bairro) ||', CEP:' || trim(e.cep) || ', ' || trim(e.localidade) || ', ' || trim(e.uf)
      FROM enderecocorreio e
      INNER JOIN pessoa_enderecocorreio pe
      ON e.id = pe.enderecoscorreio_id
      WHERE pe.pessoa_id = pessoa.id
      and (e.tipoendereco ='RESIDENCIAL' OR e.tipoendereco ='COMERCIAL' OR e.tipoendereco = 'CORRESPONDENCIA')
      AND rownum =1
  ) AS endereco,
  COALESCE(bci.inscricaocadastral, cmc.inscricaocadastral, TO_CHAR(bcr.codigo), ceasa.numeroContrato, rendas.numeroContrato) AS inscricao,
  lote.codigolote AS lotebci,
  quadra.descricao AS quadrabci,
  bcr.nomepropriedade AS nomerural,
  bcr.localizacaolote AS localizacaorural,
  bcr.numeroincra,
  InitCap(REPLACE(cmc.classificacaoatividade , '_', ' ')) AS classificacaocmc,
  natjur.descricao AS cmc_naturezajuridica,
  divida.descricao                                        AS divida,
  exercicio.ano                                           AS exercicio,
  parcela.sequenciaparcela parcela,
  op.promocional                            AS cota_unica,
  calculo.subdivida                         AS SD,
  TO_CHAR(parcela.vencimento, 'DD/MM/YYYY') AS vencimento,
  dam.numerodam,
  parcela.id                                           AS id_parcela,
  InitCap(REPLACE(situacao.situacaoparcela, '_', ' ')) AS situacao,
  TO_CHAR(situacao.datalancamento, 'DD/MM/YYYY')       AS data_operacao,
  CASE
    WHEN parcelamento.id IS NOT NULL
    THEN 'Parcelamento nº.'
      || parcelamento.numero
    WHEN da.id IS NOT NULL
    THEN 'Ins. D.A. nº.'
      || da.numero
    WHEN daoriginal.id IS NOT NULL
    THEN 'Ins. D.A. nº.'
      || daoriginal.numero
    WHEN lotebaixa.id IS NOT NULL
    THEN 'Arrecadação nº.'
      || lotebaixa.CODIGOLOTE
      || ' '
      || banco.descricao
    WHEN processodebito.id IS NOT NULL
    THEN 'Proc. Débitos nº.'
      || processodebito.CODIGO
    ELSE 'Processo de Calc. de ' || divida.descricao
  END AS descricao_operacao,
  coalesce(
  CASE
    WHEN situacao.situacaoparcela = 'PAGO'
    THEN itemlote.valorpago
    ELSE
    ((situacao.saldo/
        coalesce((select m.valor from Moeda m inner join indiceeconomico indice on indice.id = m.indiceeconomico_id where m.mes = TO_NUMBER(TO_CHAR(parcela.dataRegistro,'MM')) and m.ano = exercicio.ano and indice.descricao = 'UFM'),1))
 * coalesce((select m.valor from Moeda m inner join indiceeconomico indice on indice.id = m.indiceeconomico_id where m.mes = TO_NUMBER(TO_CHAR(current_date,'MM')) and m.ano = TO_NUMBER(TO_CHAR(current_date,'yyyy')) and indice.descricao = 'UFM'),1))
  END, 0) AS valor,
  coalesce(
  CASE
    WHEN situacao.situacaoparcela = 'PAGO'
    THEN COALESCE(itemlote.juropago, 0)
    ELSE COALESCE(ac.juros, 0)
  END, 2) AS juros,
  coalesce(
  CASE
    WHEN situacao.situacaoparcela = 'PAGO'
    THEN COALESCE(itemlote.multapago, 0)
    ELSE COALESCE(ac.multa,0)
  END, 0)  AS multa,
   CASE
    WHEN situacao.situacaoparcela = 'PAGO'
    then 0 else
  (select coalesce(sum(item.valor * (desconto.porcentagemDesconto/100)),0) from descontoitemparcela desconto inner join itemparcelavalordivida item on item.id = desconto.itemparcelavalordivida_id where item.parcelavalordivida_id = parcela.id and desconto.porcentagemdesconto >0) end as desconto,
  PARCELA.DIVIDAATIVA
 FROM parcelavalordivida parcela
LEFT JOIN temp_acrescimos ac ON ac.parcela_id = parcela.id
INNER JOIN valordivida ON valordivida.id = parcela.valordivida_id
INNER JOIN situacaoparcelavalordivida situacao ON situacao.parcela_id = parcela.id
INNER JOIN calculo ON calculo.id = valordivida.calculo_id
INNER JOIN cadastro ON cadastro.id = calculo.cadastro_id
INNER JOIN divida ON divida.id = valordivida.divida_id
INNER JOIN exercicio ON exercicio.id = valordivida.exercicio_id
INNER JOIN opcaopagamento op ON op.id = parcela.opcaopagamento_id
INNER JOIN calculopessoa cp ON cp.calculo_id = calculo.id
INNER JOIN pessoa on pessoa.id = cp.pessoa_id
LEFT JOIN pessoafisica pf ON pf.id = pessoa.id
LEFT JOIN pessoajuridica pj ON pj.id = pessoa.id
LEFT JOIN cadastroimobiliario bci ON bci.id = cadastro.id
LEFT JOIN lote ON lote.id = bci.lote_id
LEFT JOIN quadra ON quadra.id = lote.quadra_id
LEFT JOIN cadastroeconomico cmc ON cmc.id = cadastro.id
LEFT JOIN naturezajuridica natjur ON cmc.naturezajuridica_id = natjur.id
LEFT JOIN cadastrorural bcr ON bcr.id = cadastro.id
LEFT JOIN contratorendaspatrimoniais rendas ON rendas.id = cadastro.id
LEFT JOIN contratoceasa ceasa ON ceasa.id = cadastro.id
LEFT JOIN processoparcelamento parcelamento ON parcelamento.id = calculo.id
LEFT JOIN iteminscricaodividaativa itemda ON itemda.id = calculo.id
LEFT JOIN inscricaodividaativa da ON da.id = itemda.inscricaodividaativa_id
LEFT JOIN inscricaodividaparcela idporiginal ON idporiginal.parcelavalordivida_id = parcela.id
LEFT JOIN iteminscricaodividaativa itemdaoriginal ON itemdaoriginal.id = idporiginal.iteminscricaodividaativa_id
LEFT JOIN inscricaodividaativa daoriginal ON daoriginal.id = itemdaoriginal.inscricaodividaativa_id
LEFT JOIN itemdam itemdam on itemdam.parcela_id = parcela.id
LEFT JOIN dam dam ON itemdam.dam_id = dam.id
LEFT JOIN itemlotebaixa itemlote ON itemlote.dam_id = dam.id
LEFT JOIN lotebaixa ON lotebaixa.id = itemlote.lotebaixa_id
LEFT JOIN banco ON banco.id = lotebaixa.banco_id
LEFT JOIN itemprocessodebito ON itemprocessodebito.parcela_id = parcela.id
LEFT JOIN processodebito ON processodebito.id = itemprocessodebito.processodebito_id

WHERE situacao.id    =
  (SELECT MAX(s.id)
      FROM situacaoparcelavalordivida s
   WHERE s.parcela_id = situacao.parcela_id)
   AND (dam.id is null or dam.situacao = 'ABERTO')
$P!{CONDICAO}
ORDER BY COALESCE(pf.nome, pj.razaosocial),
  COALESCE(bci.inscricaocadastral, cmc.inscricaocadastral, TO_CHAR(bcr.codigo), ceasa.numeroContrato, rendas.numeroContrato),
  exercicio.ano ,
  divida.descricao ,
  op.promocional ,
  to_number(regexp_substr(parcela.sequenciaparcela,'[[:digit:]]*')),
  calculo.subdivida ,
  parcela.vencimento,
  situacao.id]]>
	</queryString>
