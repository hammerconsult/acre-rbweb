	<field name="VALORAJUSTE" class="java.math.BigDecimal"/>
	<field name="QUANTIDADE" class="java.math.BigDecimal"/>
	<variable name="TOTAL_RECONHECIDO" class="java.math.BigDecimal" calculation="Sum">
