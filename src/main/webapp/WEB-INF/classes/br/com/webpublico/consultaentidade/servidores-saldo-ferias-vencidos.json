    "from": "from CONTRATOFP obj\r\n         inner join VINCULOFP vin on obj.ID = vin.ID\r\n         inner join PERIODOAQUISITIVOFL pa on obj.ID = pa.CONTRATOFP_ID\r\n         inner join BASECARGO on pa.BASECARGO_ID = BASECARGO.ID\r\n         inner join BASEPERIODOAQUISITIVO base on BASECARGO.BASEPERIODOAQUISITIVO_ID = base.ID\r\n         inner join cargo on obj.CARGO_ID = CARGO.ID\r\n         inner join matriculafp mat on vin.MATRICULAFP_ID = mat.ID\r\n         inner join pessoaFisica pf on mat.PESSOA_ID = pf.ID\r\n         inner join vwhierarquiaadministrativa ho on ho.SUBORDINADA_ID = vin.UNIDADEORGANIZACIONAL_ID\r\nwhere base.TIPOPERIODOAQUISITIVO = 'FERIAS'\r\nand pa.SALDO > 0\r\nand (pa.id not in (select sugestao.PERIODOAQUISITIVOFL_ID\r\n                     from SUGESTAOFERIAS sugestao\r\n                     where sugestao.PERIODOAQUISITIVOFL_ID = pa.id)\r\n    or pa.SALDO > (select trunc(sugestao.DATAFIM) - trunc(sugestao.DATAINICIO) + 1\r\n                   from SUGESTAOFERIAS sugestao\r\n                   where sugestao.PERIODOAQUISITIVOFL_ID = pa.id)\r\n    or pa.id not in (select sf.PERIODOAQUISITIVOFL_ID\r\n                     from SUGESTAOFERIAS sf\r\n                     where sf.PERIODOAQUISITIVOFL_ID not in (select concessao.PERIODOAQUISITIVOFL_ID\r\n                                                             from CONCESSAOFERIASLICENCA concessao\r\n                                                             where concessao.PERIODOAQUISITIVOFL_ID = sf.PERIODOAQUISITIVOFL_ID)\r\n                       and current_date > sf.DATAFIM\r\n                       and sf.PERIODOAQUISITIVOFL_ID = pa.id))\r\nand $DATA_OPERACAO between ho.INICIOVIGENCIA and coalesce(ho.FIMVIGENCIA, $DATA_OPERACAO)\r\norder by pf.nome\r\n",
