				</textElement>
				<textFieldExpression><![CDATA[br.com.webpublico.enums.SituacaoParcela.valueOf($F{SITUACAOPARCELA}).getDescricao() + " " + ($F{SITUACAOEMABERTO} != null? $F{SITUACAOEMABERTO} : "") + ": "]]></textFieldExpression>
