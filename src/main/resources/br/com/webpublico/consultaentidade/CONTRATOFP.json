    "from": "from vinculofp vinculo\r\n         inner join contratofp contrato on vinculo.ID = contrato.ID\r\n         inner join contratofp contrato on vinculo.ID = contrato.ID\r\n         inner join matriculafp mat on vinculo.MATRICULAFP_ID = mat.ID\r\n         inner join pessoafisica pf on mat.PESSOA_ID = pf.ID\r\n         inner join cargo on contrato.CARGO_ID = cargo.id\r\n         inner join TipoRegime on contrato.TIPOREGIME_ID = TIPOREGIME.ID\r\n         inner join modalidadecontratofp modalidade on contrato.MODALIDADECONTRATOFP_ID = modalidade.ID\r\n         inner join JORNADADETRABALHO jornada on contrato.JORNADADETRABALHO_ID = jornada.ID\r\n         inner join atolegal ato on contrato.ATOLEGAL_ID = ato.ID\r\n         inner join EXERCICIO exercicioato on ato.EXERCICIO_ID = exercicioato.id\r\n         inner join ESFERAGOVERNO esfera on ato.ESFERAGOVERNO_ID = esfera.ID\r\n         inner join SITUACAOFUNCIONAL situacao on contrato.SITUACAOFUNCIONAL_ID = situacao.ID\r\n         left join cbo on vinculo.CBO_ID = CBO.ID\r\n         left join ContaCorrenteBancaria conta on vinculo.CONTACORRENTE_ID = conta.ID\r\n         left join agencia on conta.AGENCIA_ID = AGENCIA.ID\r\n         left join banco on agencia.BANCO_ID = banco.id\r\n         where (exists (select 1\r\n                   from UNIDADECARGO uni\r\n                            inner join hierarquiaorganizacional ho on ho.SUBORDINADA_ID = uni.UNIDADEORGANIZACIONAL_ID\r\n                   where uni.CARGO_ID = cargo.id\r\n                     and ho.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n                   and $DATA_OPERACAO between trunc(ho.INICIOVIGENCIA) and coalesce (trunc(ho.FIMVIGENCIA), $DATA_OPERACAO)\r\n                   and ho.INDICEDONO = 1 and substr(ho.codigo,1,3) in (select distinct substr(hierarquia.codigo,1,3)\r\n                   from hierarquiaorganizacional hierarquia\r\n                   inner join usuariounidadeorganizacio uu\r\n                   on uu.unidadeorganizacional_id = hierarquia.subordinada_id\r\n                   inner join usuariosistema u on u.id = uu.usuariosistema_id\r\n                   where u.id = $USUARIO_ID\r\n                   and hierarquia.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n                   and hierarquia.NIVELNAENTIDADE = 2\r\n                   and $DATA_OPERACAO between trunc(hierarquia.INICIOVIGENCIA) and coalesce (trunc(hierarquia.FIMVIGENCIA), $DATA_OPERACAO))) or\r\n           exists (select 1\r\n                   from UNIDADECARGO uni\r\n                   where uni.CARGO_ID = cargo.id\r\n                     and uni.UNIDADEORGANIZACIONAL_ID in (select distinct ho.SUBORDINADA_ID\r\n                                                          from hierarquiaorganizacional ho\r\n                                                                   inner join usuariounidadeorganizacio uu\r\n                                                                              on uu.unidadeorganizacional_id = ho.subordinada_id\r\n                                                                   inner join usuariosistema u on u.id = uu.usuariosistema_id\r\n                                                          where u.id = $USUARIO_ID\r\n                                                            and ho.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n                                                            and ho.NIVELNAENTIDADE = 2\r\n                                                            and $DATA_OPERACAO between trunc(ho.INICIOVIGENCIA) and coalesce (trunc(ho.FIMVIGENCIA), $DATA_OPERACAO))))",
