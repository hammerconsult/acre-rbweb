    "from": "from Estagiario obj\r\n         inner join pessoaFisica pf\r\n                    on obj.PESSOAFISICA_ID = pf.ID\r\n         inner join CONTRATOFP cont on obj.ID = cont.ID\r\n         inner join MATRICULAFP mat on pf.ID = mat.PESSOA_ID\r\n         inner join VINCULOFP v on cont.ID = v.ID\r\n         left join cargo on cont.CARGO_ID = cargo.ID\r\n         left join ModalidadeContratoFP modalidade on cont.MODALIDADECONTRATOFP_ID = modalidade.ID\r\n         left join pessoajuridica pj on obj.PESSOAJURIDICA_ID = pj.ID\r\n         left join TipoRegime regime on cont.TIPOREGIME_ID = regime.ID\r\n         where (cont.CARGO_ID is null or exists (select 1\r\n          from UNIDADECARGO uni\r\n          inner join hierarquiaorganizacional ho on ho.SUBORDINADA_ID = uni.UNIDADEORGANIZACIONAL_ID\r\n          where uni.CARGO_ID = cargo.id\r\n          and ho.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n          and $DATA_OPERACAO between trunc(ho.INICIOVIGENCIA) and coalesce (trunc(ho.FIMVIGENCIA), $DATA_OPERACAO)\r\n          and ho.INDICEDONO = 1 and substr(ho.codigo,1,3) in (select distinct substr(hierarquia.codigo,1,3)\r\n          from hierarquiaorganizacional hierarquia\r\n          inner join usuariounidadeorganizacio uu\r\n          on uu.unidadeorganizacional_id = hierarquia.subordinada_id\r\n          inner join usuariosistema u on u.id = uu.usuariosistema_id\r\n          where u.id = $USUARIO_ID\r\n          and hierarquia.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n          and hierarquia.NIVELNAENTIDADE = 2\r\n          and $DATA_OPERACAO between trunc(hierarquia.INICIOVIGENCIA) and coalesce (trunc(hierarquia.FIMVIGENCIA), $DATA_OPERACAO))) or\r\n          exists (select 1\r\n          from UNIDADECARGO uni\r\n          where uni.CARGO_ID = cargo.id\r\n          and uni.UNIDADEORGANIZACIONAL_ID in (select distinct ho.SUBORDINADA_ID\r\n          from hierarquiaorganizacional ho\r\n          inner join usuariounidadeorganizacio uu\r\n          on uu.unidadeorganizacional_id = ho.subordinada_id\r\n          inner join usuariosistema u on u.id = uu.usuariosistema_id\r\n          where u.id = $USUARIO_ID\r\n          and ho.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n          and ho.NIVELNAENTIDADE = 2\r\n          and $DATA_OPERACAO between trunc(ho.INICIOVIGENCIA) and coalesce (trunc(ho.FIMVIGENCIA), $DATA_OPERACAO))))",
