    "from": "from CargoConfianca cargo\r\n    inner join vinculofp vinculo on vinculo.ID = cargo.CONTRATOFP_ID\r\n    inner join MATRICULAFP mat on vinculo.MATRICULAFP_ID = mat.ID\r\n    inner join pessoafisica pf on mat.PESSOA_ID = pf.ID\r\n    left join OpcaoSalarialCC opcao on cargo.OPCAOSALARIALCC_ID = opcao.ID\r\n    left join EnquadramentoCC enquadramento on cargo.ID = enquadramento.CARGOCONFIANCA_ID\r\n    left join ProgressaoPCS progressao on enquadramento.PROGRESSAOPCS_ID = progressao.ID\r\n    where (exists (select 1\r\n                       from UNIDADECARGO uni\r\n                                inner join hierarquiaorganizacional ho on ho.SUBORDINADA_ID = uni.UNIDADEORGANIZACIONAL_ID\r\n                       where uni.CARGO_ID = cargo.CARGO_ID\r\n                         and ho.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n                       and $DATA_OPERACAO between trunc(ho.INICIOVIGENCIA) and coalesce (trunc(ho.FIMVIGENCIA), $DATA_OPERACAO)\r\n                       and ho.INDICEDONO = 1 and substr(ho.codigo,1,3) in (select distinct substr(hierarquia.codigo,1,3)\r\n                       from hierarquiaorganizacional hierarquia\r\n                       inner join usuariounidadeorganizacio uu\r\n                       on uu.unidadeorganizacional_id = hierarquia.subordinada_id\r\n                       inner join usuariosistema u on u.id = uu.usuariosistema_id\r\n                       where u.id = $USUARIO_ID\r\n                       and hierarquia.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n                       and hierarquia.NIVELNAENTIDADE = 2\r\n                       and $DATA_OPERACAO between trunc(hierarquia.INICIOVIGENCIA) and coalesce (trunc(hierarquia.FIMVIGENCIA), $DATA_OPERACAO))) or\r\n               exists (select 1\r\n                       from UNIDADECARGO uni\r\n                       where uni.CARGO_ID = cargo.CARGO_ID\r\n                         and uni.UNIDADEORGANIZACIONAL_ID in (select distinct ho.SUBORDINADA_ID\r\n                                                              from hierarquiaorganizacional ho\r\n                                                                       inner join usuariounidadeorganizacio uu\r\n                                                                                  on uu.unidadeorganizacional_id = ho.subordinada_id\r\n                                                                       inner join usuariosistema u on u.id = uu.usuariosistema_id\r\n                                                              where u.id = $USUARIO_ID\r\n                                                                and ho.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n                                                                and ho.NIVELNAENTIDADE = 2\r\n                                                                and $DATA_OPERACAO between trunc(ho.INICIOVIGENCIA) and coalesce (trunc(ho.FIMVIGENCIA), $DATA_OPERACAO))))",
