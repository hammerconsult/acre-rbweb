    "from": "from AlteracaoCargo alteracao\r\ninner join CONTRATOFPCARGO on alteracao.CONTRATOFPCARGO_ID = CONTRATOFPCARGO.ID\r\ninner join contratofp contrato on CONTRATOFPCARGO.CONTRATOFP_ID = contrato.id\r\ninner join vinculofp vinculo on contrato.ID = vinculo.ID\r\ninner join MATRICULAFP MAT on vinculo.MATRICULAFP_ID = mat.ID\r\ninner join pessoafisica pf on mat.PESSOA_ID = pf.ID\r\ninner join cargo on contrato.CARGO_ID = CARGO.ID\r\ninner join LOTACAOFUNCIONAL lotacao on vinculo.ID = lotacao.VINCULOFP_ID\r\ninner join VwHierarquiaAdministrativa VW ON LOTACAO.UNIDADEORGANIZACIONAL_ID = VW.SUBORDINADA_ID\r\ninner join ProvimentoFP provimento on alteracao.PROVIMENTOFP_ID = provimento.ID\r\nWHERE $DATA_OPERACAO   between  vw.INICIOVIGENCIA and coalesce (vw.FIMVIGENCIA, $DATA_OPERACAO  )\r\nand alteracao.data between lotacao.INICIOVIGENCIA and coalesce(lotacao.FINALVIGENCIA, alteracao.data)\r\nand (exists (select 1\r\n    from UNIDADECARGO uni\r\n    inner join hierarquiaorganizacional ho on ho.SUBORDINADA_ID = uni.UNIDADEORGANIZACIONAL_ID\r\n    where uni.CARGO_ID = cargo.id\r\n    and ho.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n    and $DATA_OPERACAO between trunc(ho.INICIOVIGENCIA) and coalesce (trunc(ho.FIMVIGENCIA), $DATA_OPERACAO)\r\n    and ho.INDICEDONO = 1 and substr(ho.codigo,1,3) in (select distinct substr(hierarquia.codigo,1,3)\r\n    from hierarquiaorganizacional hierarquia\r\n    inner join usuariounidadeorganizacio uu\r\n    on uu.unidadeorganizacional_id = hierarquia.subordinada_id\r\n    inner join usuariosistema u on u.id = uu.usuariosistema_id\r\n    where u.id = $USUARIO_ID\r\n    and hierarquia.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n    and hierarquia.NIVELNAENTIDADE = 2\r\n    and $DATA_OPERACAO between trunc(hierarquia.INICIOVIGENCIA) and coalesce (trunc(hierarquia.FIMVIGENCIA), $DATA_OPERACAO))) or\r\n    exists (select 1\r\n    from UNIDADECARGO uni\r\n    where uni.CARGO_ID = cargo.id\r\n    and uni.UNIDADEORGANIZACIONAL_ID in (select distinct ho.SUBORDINADA_ID\r\n    from hierarquiaorganizacional ho\r\n    inner join usuariounidadeorganizacio uu\r\n    on uu.unidadeorganizacional_id = ho.subordinada_id\r\n    inner join usuariosistema u on u.id = uu.usuariosistema_id\r\n    where u.id = $USUARIO_ID\r\n    and ho.tipohierarquiaorganizacional = $TIPO_HIERARQUIA_ADM\r\n    and ho.NIVELNAENTIDADE = 2\r\n    and $DATA_OPERACAO between trunc(ho.INICIOVIGENCIA) and coalesce (trunc(ho.FIMVIGENCIA), $DATA_OPERACAO))))",
